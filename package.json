{
  "name": "memory-visualiser",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "compile": "wat2js reverse.wat > reverse-wasm.js",
    "build": "bankai build index.js --base '/memory-visualiser'",
    "create": "choo-scaffold",
    "inspect": "bankai inspect index.js",
    "start": "bankai start index.js",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "standard": {
    "globals": [ "WebAssembly", "FormData", "atob" ]
  },
  "dependencies": {
    "choo": "^6.7.0",
    "choo-devtools": "^2.3.3",
    "new-array": "^1.0.0",
    "sheetify": "^7.1.0",
    "tachyons": "^4.9.1"
  },
  "devDependencies": {
    "bankai": "^9.7.1",
    "choo-scaffold": "^1.1.2",
    "dependency-check": "^3.0.0",
    "standard": "^11.0.0",
    "wat2js": "^1.1.1"
  }
}
